{
  "name": "nodejs-sample",
  "version": "0.0.1",
  "description": "Starter Node.js sample project",
  "main": "dst",
  "directories": {
    "test": "src/test/"
  },
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,es2017,stage-0\"",
    "build": "babel src -s -D -d dst --presets es2015,es2017,stage-0",
    "start": "node dst",
    "prestart": "npm run -s build",
    "test": "mocha --compilers js:babel-register src/test/user.spec.js",
    "_test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive $(find test -name '*.spec.js')"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellflat/nodejs-sample.git"
  },
  "author": "wellflat",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-validator": "^4.2.1",
    "knex": "^0.13.0",
    "morgan": "^1.8.0",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "mocha": "^4.0.1",
    "mock-knex": "^0.3.9",
    "nodemon": "^1.9.2",
    "power-assert": "^1.4.4",
    "source-map-support": "^0.5.0"
  },
  "bugs": {
    "url": "https://github.com/wellflat/nodejs-sample"
  },
  "keywords": [
    "node"
  ]
}
